<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
    <VBox prefHeight="400.0" prefWidth="400.0">
        <GridPane.margin>
            <Insets/>
        </GridPane.margin>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <Label fx:id="classNameLabel" prefHeight="25.0"/>
        <HBox>
            <VBox.margin>
                <Insets top="5.0"/>
            </VBox.margin>
            <Label fx:id="professorInChargeLabel" prefHeight="25.0"/>
            <Button fx:id="professorInChargeInfoButton" mnemonicParsing="false"
                    onAction="#viewProfessorInChargeInfoAction" text="Voir infos">
                <HBox.margin>
                    <Insets left="5.0"/>
                </HBox.margin>
            </Button>
        </HBox>
        <Button fx:id="viewEUButton" mnemonicParsing="false" onAction="#viewEUAction" text="Voir les UE">
            <VBox.margin>
                <Insets top="5.0"/>
            </VBox.margin>
        </Button>
        <ListView fx:id="studentsListView" prefHeight="400.0">
            <VBox.margin>
                <Insets top="5.0"/>
            </VBox.margin>
        </ListView>
        <Button fx:id="viewInfoStudentButton" mnemonicParsing="false" onAction="#viewInfoStudentAction"
                text="Voir infos élève">
            <VBox.margin>
                <Insets top="5.0"/>
            </VBox.margin>
        </Button>
    </VBox>
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
</GridPane>
