<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <Label fx:id="classBatchNameLabel" prefHeight="25.0"/>
    <HBox>
        <VBox.margin>
            <Insets top="5.0"/>
        </VBox.margin>
        <Label prefHeight="25.0" text="Professeur en charge : "/>
        <Label fx:id="professorInChargeLabel" prefHeight="25.0"/>
        <Button fx:id="professorInChargeInfoButton" mnemonicParsing="false" onAction="#professorInChargeInfoAction"
                text="Voir profile">
            <HBox.margin>
                <Insets left="5.0"/>
            </HBox.margin>
        </Button>
    </HBox>
    <Button fx:id="euInfoButton" mnemonicParsing="false" onAction="#euInfoAction" text="Voir les UEs">
        <VBox.margin>
            <Insets top="5.0"/>
        </VBox.margin>
    </Button>
    <ListView fx:id="studentsListView" prefHeight="400.0">
        <VBox.margin>
            <Insets top="5.0"/>
        </VBox.margin>
    </ListView>
    <Button fx:id="studentInfoButton" mnemonicParsing="false" onAction="#studentInfoAction" text="Voir profile élève">
        <VBox.margin>
            <Insets top="5.0"/>
        </VBox.margin>
    </Button>
</VBox>
